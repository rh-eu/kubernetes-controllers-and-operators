{
    "apiVersion":"apps/v1",
    "kind":"Deployment",
    "metadata":{
      "annotations":{
        
      },
      "labels":{
        "app":"hello-app"
      },
      "name":"hello-app",
      "namespace":"enforce-annotations"
    },
    "spec":{
      "selector":{
        "matchLabels":{
          "app":"hello-app"
        }
      },
      "template":{
        "metadata":{
          "annotations":{
            "k8s.questionable.services/id":"abc",
            "mifomm.eu/name":"RH"
          },
          "labels":{
            "app":"hello-app"
          }
        },
        "spec":{
          "containers":[
            {
              "image":"mifomm/goservd-amd64:0.0.7",
              "name":"hello-app",
              "ports":[
                {
                  "containerPort":5000
                }
              ],
              "resources":{
                "limits":{
                  "cpu":"25m",
                  "memory":"263M"
                },
                "requests":{
                  "cpu":"25m",
                  "memory":"263M"
                }
              }
            }
          ]
        }
      }
    }
  }